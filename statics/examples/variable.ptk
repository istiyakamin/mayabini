/* ভ্যারিয়েবল ডিফাইন করা
 *
 * 'ধরি' কিওয়ার্ড এর মাধ্যমে ভ্যারিয়েবল ডিক্লেয়ার করতে হবে।
 */

ধরি ক = ১০;           // নাম্বার
ধরি খ = ২ * ৫ + ২.৬;  // দশমিক নাম্বার
ধরি ব = সত্য;          // বুলিয়ান
ধরি এ = [১,৩,৪];      // এ্যারে

/* অবজেক্ট ডিফাইন করা */
ধরি অ = {
  নাম : "ইকরাম হোসেন",
  শখ : "আকাশ কুসুম স্বপ্ন দেখা"
}

দেখাও(ক, খ, এ, অ);
